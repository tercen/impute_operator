{
  "name": "Impute",
  "description": "Replaces missing data by the column mean or an arbitrary value.",
  "tags": [
    "preprocessing"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/impute_operator"
  ],
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "method",
      "defaultValue": "constant",
      "values": [
        "constant",
        "mean_per_column",
        "mean_per_row"
      ],
      "description": "Method to be used for imputation"
    },
    {
      "kind": "DoubleProperty",
      "name": "value",
      "defaultValue": 0,
      "description": "Value to be used for imputation (constant method)"
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Optional row factor.",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Optional column factor.",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Data",
                "type": "numeric",
                "description": "Input data to be imputed, per cell",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}
